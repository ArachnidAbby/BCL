define main() {
    x = 6;
    test = [0,0,0,0];
    println(test[x]);
    i = 0;
    loop_amount = 200;
    while i < loop_amount {
        i = i+1;
        fizz = is_multiple(i, 3);
        buzz = is_multiple(i, 5)*2;
        println(fizz + buzz);
    }
}

define is_multiple(value: i32, divider: i32) -> bool {
    return (value % divider) == 0;
}

define test(x: i32[3]) -> i32[3] {
    return x;
}