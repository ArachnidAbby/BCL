define main() {
    x = 6;
    test = [0; 10];
    println(test[x]);
    test[x] = 8;
    // x = "helloworld";
    // println(x);
    loop_amount = 20;
    for i in 0..loop_amount {
        fizz = is_multiple(&i, 3);
        buzz = is_multiple(&i, 5)*2;
        println(fizz + buzz);
    }
}

define is_multiple(value: &i32, divider: i32) -> &bool {
    value += 2;
    return (value % divider) == 0;
}

